<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignUpController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <GridPane prefHeight="377.0" prefWidth="500.0" GridPane.rowIndex="3">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints hgrow="SOMETIMES" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="45.0" />
                <RowConstraints minHeight="10.0" prefHeight="45.0" />
            </rowConstraints>
            <children>
                <TextField fx:id="firstName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="lastName" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="username" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <PasswordField fx:id="pw1" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <PasswordField fx:id="pw2" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                <TextField fx:id="phone" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                <DatePicker fx:id="dob" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                <TextField fx:id="gender" GridPane.columnIndex="1" GridPane.rowIndex="9" />
                <Button fx:id="photo" mnemonicParsing="false" onMouseClicked="#selectPhoto" text="Browse..." GridPane.columnIndex="1" GridPane.rowIndex="10" />


                <Label contentDisplay="RIGHT" text="First Name" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Last Name" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Username" GridPane.halignment="RIGHT" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Password" GridPane.halignment="RIGHT" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Password Again" GridPane.halignment="RIGHT" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Email" GridPane.halignment="RIGHT" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Phone" GridPane.halignment="RIGHT" GridPane.rowIndex="7">
