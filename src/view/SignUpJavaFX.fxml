                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="DOB" GridPane.halignment="RIGHT" GridPane.rowIndex="8">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Gender" GridPane.halignment="RIGHT" GridPane.rowIndex="9">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>
                <Label contentDisplay="RIGHT" text="Photo" GridPane.halignment="RIGHT" GridPane.rowIndex="10">
                    <GridPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </GridPane.margin>
                </Label>

                <Button mnemonicParsing="false" onMouseClicked="#signUp" prefHeight="27.0" prefWidth="79.0" style="-fx-background-color: #6cc;" text="Sign Up" GridPane.columnIndex="1" GridPane.rowIndex="11" />
                <Button mnemonicParsing="false" onMouseClicked="#openLoginView" prefHeight="27.0" prefWidth="75.0" style="-fx-background-color: #eee;" text="Login" GridPane.halignment="CENTER" GridPane.rowIndex="11" GridPane.valignment="CENTER" />
            <Label fx:id="photo_url" contentDisplay="RIGHT" text="Select a Photo" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="10" />
            </children>
            <GridPane.margin>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
            </GridPane.margin>
        </GridPane>
        <Label prefHeight="17.0" prefWidth="337.0" text="Sign Up">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin>
            <font>
                <Font name="System Bold" size="13.0" />
            </font>
        </Label>
        <Label fx:id="alert" GridPane.rowIndex="1" />
    </children>
    <stylesheets>
        <URL value="@mystyle.css" />
    </stylesheets>
</GridPane>
